<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:panels="clr-namespace:StakeforgeEditor.Panels"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate DataType="{x:Type panels:EntitiesViewModel}">
        <panels:EntitiesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type panels:ResourcesViewModel}">
        <panels:ResourcesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type panels:ConsoleViewModel}">
        <panels:ConsoleView/>
    </DataTemplate>

</ResourceDictionary>